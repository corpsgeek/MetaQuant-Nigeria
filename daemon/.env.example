# MetaQuant Daemon Environment Variables
# Copy this to .env and fill in your values

# Telegram Bot (required)
# Get from @BotFather on Telegram
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHAT_ID=your_chat_id_here

# TradingView (optional, for premium data)
TRADINGVIEW_USER=your_email
TRADINGVIEW_PASS=your_password

# Groq AI (optional, for AI narratives)
GROQ_API_KEY=your_groq_api_key

# Database (leave empty for local DuckDB)
DATABASE_URL=
